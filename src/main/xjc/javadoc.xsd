			<xs:element name="final" type="xs:boolean" />
			<xs:element name="included" type="xs:boolean" />
			<xs:element name="native" type="xs:boolean" />
			<xs:element name="synchronized" type="xs:boolean" />
			<xs:element name="static" type="xs:boolean" />
			<xs:element name="varArgs" type="xs:boolean" />
			<xs:element name="signature" type="xs:string" minOccurs="0" />
			<xs:element name="parameters" type="methodParameter"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="return" type="typeInfo" minOccurs="0" />
			<xs:element name="throws" type="typeInfo" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="annotations" type="annotationInstance"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="methodParameter">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="type" type="typeInfo" minOccurs="0" />
			<xs:element name="annotations" type="annotationInstance"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="field">
		<xs:sequence>
			<xs:element name="type" type="typeInfo" minOccurs="0" />
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="qualifiedName" type="xs:string"
				minOccurs="0" />
			<xs:element name="comment" type="xs:string" minOccurs="0" />
			<xs:element name="scope" type="xs:string" minOccurs="0" />
			<xs:element name="volatile" type="xs:boolean" />
			<xs:element name="transient" type="xs:boolean" />
			<xs:element name="static" type="xs:boolean" />
			<xs:element name="final" type="xs:boolean" />
			<xs:element name="constant" type="xs:string" minOccurs="0" />
			<xs:element name="annotations" type="annotationInstance"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="typeInfo">
		<xs:sequence>
			<xs:element name="qualifiedName" type="xs:string"
				minOccurs="0" />
			<xs:element name="wildcard" type="wildcard" minOccurs="0" />
			<xs:element name="generics" type="typeInfo" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="dimension" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="typeParameter">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="bounds" type="xs:string" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="wildcard">
		<xs:sequence>
			<xs:element name="extendsBounds" type="typeInfo"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="superBounds" type="typeInfo" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="scope">
		<xs:restriction base="xs:string">
			<xs:enumeration value="public" />
			<xs:enumeration value="protected" />
			<xs:enumeration value="private" />
			<xs:enumeration value="" />
		</xs:restriction>
	</xs:simpleType>

</xs:schema>