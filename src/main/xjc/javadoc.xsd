<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:element name="root">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="packages" type="package" minOccurs="0"
					maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="package">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="comment" type="xs:string" minOccurs="0" />
			<xs:element name="annotations" type="annotation"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="enums" type="enum" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="interfaces" type="interface" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="classes" type="class" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="annotation">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="qualifiedName" type="xs:string"
				minOccurs="0" />
			<xs:element name="comment" type="xs:string" minOccurs="0" />
			<xs:element name="scope" type="scope" minOccurs="0" />
			<xs:element name="included" type="xs:boolean" />
			<xs:element name="elements" type="annotationElement"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="annotations" type="annotationInstance"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="annotationElement">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="qualifiedName" type="xs:string"
				minOccurs="0" />
			<xs:element name="type" type="xs:string" minOccurs="0" />
			<xs:element name="defaultValue" type="xs:string"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="annotationInstance">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="qualifiedName" type="xs:string"
				minOccurs="0" />
			<xs:element name="arguments" type="annotationArgument"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="annotationArgument">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="value" type="xs:string" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="type" type="xs:string" minOccurs="0" />
			<xs:element name="primitive" type="xs:boolean" />
			<xs:element name="array" type="xs:boolean" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="enum">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="qualifiedName" type="xs:string"
				minOccurs="0" />
			<xs:element name="comment" type="xs:string" minOccurs="0" />
			<xs:element name="scope" type="xs:string" minOccurs="0" />
			<xs:element name="included" type="xs:boolean" />
			<xs:element name="extends" type="typeInfo" minOccurs="0" />
			<xs:element name="implements" type="typeInfo" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="constants" type="enumConstant"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="annotations" type="annotationInstance"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="enumConstant">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="comment" type="xs:string" minOccurs="0" />
			<xs:element name="annotations" type="annotationInstance"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="interface">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="qualifiedName" type="xs:string"
				minOccurs="0" />
			<xs:element name="comment" type="xs:string" minOccurs="0" />
			<xs:element name="scope" type="xs:string" minOccurs="0" />
			<xs:element name="included" type="xs:boolean" />
			<xs:element name="generics" type="typeParameter"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="extends" type="typeInfo" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="methods" type="method" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="annotations" type="annotationInstance"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="class">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="qualifiedName" type="xs:string"
				minOccurs="0" />
			<xs:element name="comment" type="xs:string" minOccurs="0" />
			<xs:element name="scope" type="xs:string" minOccurs="0" />
			<xs:element name="abstract" type="xs:boolean" />
			<xs:element name="error" type="xs:boolean" />
			<xs:element name="exception" type="xs:boolean" />
			<xs:element name="externalizable" type="xs:boolean" />
			<xs:element name="included" type="xs:boolean" />
			<xs:element name="serializable" type="xs:boolean" />
			<xs:element name="generics" type="typeParameter"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="extends" type="typeInfo" minOccurs="0" />
			<xs:element name="implements" type="typeInfo" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="constructors" type="constructor"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="methods" type="method" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="annotations" type="annotationInstance"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="fields" type="field" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="constructor">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="qualifiedName" type="xs:string"
				minOccurs="0" />
			<xs:element name="comment" type="xs:string" minOccurs="0" />
			<xs:element name="scope" type="xs:string" minOccurs="0" />
			<xs:element name="final" type="xs:boolean" />
			<xs:element name="included" type="xs:boolean" />
			<xs:element name="native" type="xs:boolean" />
			<xs:element name="synchronized" type="xs:boolean" />
			<xs:element name="static" type="xs:boolean" />
